<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="Rewrite Clientes">
                    <match url="^(.*)$" />
                    <conditions trackAllCaptures="true">
                        <add input="{REQUEST_FILENAME}" matchType="IsFile" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="clientesHorse.dll/{R:1}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
